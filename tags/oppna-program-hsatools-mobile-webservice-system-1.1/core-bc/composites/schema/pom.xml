  <name>${project.artifactId}</name>

  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jaxws-maven-plugin</artifactId>
        <!-- Dependency fixes m2eclipse issues -->
        <dependencies>
          <dependency>
