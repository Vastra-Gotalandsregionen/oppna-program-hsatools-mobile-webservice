            <configuration>
              <wsdlDirectory>${basedir}/src/main/resources/</wsdlDirectory>
              <wsdlLocation>https://kivws.vgregion.se:10020/services?wsdl</wsdlLocation>
              <wsdlFiles>
                <wsdlFile>kivws.wsdl</wsdlFile>
